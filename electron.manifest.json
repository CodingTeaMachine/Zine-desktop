{
  "executable": "Zine",
  "splashscreen": {
    "imageFile": ""
  },
  "name": "Zine",
  "author": "",
  "singleInstance": false,
  "environment": "Development",
  "build": {
    "appId": "com.Zine.app",
    "productName": "Zine",
    "copyright": "Copyright Â© 2020",
    "buildVersion": "1.0.0",
    "compression": "maximum",
    "directories": {
      "output": "../../../bin/Desktop"
    },
    "extraResources": [
      {
        "from": "./bin",
        "to": "bin",
        "filter": [ "**/*" ]
      }
    ],
    "files": [
      {
        "from": "./ElectronHostHook/node_modules",
        "to": "ElectronHostHook/node_modules",
        "filter": [ "**/*" ]
      },
      "**/*"
    ]
  },
  "watchOptions": {
    "includes": [ "**/*" ],
    "excludes": [ "wwwroot/images/Reading/**/*" ]
  }
}
