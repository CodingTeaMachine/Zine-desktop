{
	"$schema": "http://json.schemastore.org/launchsettings.json",
	"profiles": {
		"Electron.NET App": {
			"commandName": "Executable",
			"executablePath": "dotnet electronize",
			"commandLineArgs": "start",
			"workingDirectory": "."
		},
		"Electron.NET App Watch": {
			"commandName": "Executable",
			"executablePath": "dotnet electronize",
			"commandLineArgs": "start /watch",
			"workingDirectory": "../"
		},
		"http": {
			"commandName": "Project",
			"dotnetRunMessages": true,
			"launchBrowser": false,
			"applicationUrl": "http://localhost:5084",
			"environmentVariables": {
				"ASPNETCORE_ENVIRONMENT": "Development"
			}
		},
		"https": {
			"commandName": "Project",
			"dotnetRunMessages": true,
			"launchBrowser": false,
			"applicationUrl": "https://localhost:7173;http://localhost:5084",
			"environmentVariables": {
				"ASPNETCORE_ENVIRONMENT": "Development"
			}
		}
	}
}
